<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gov.jussantiago.jmulki.autoconsulta">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconfinder_law"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconfinder_law"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpedientesActivity" />
        <activity android:name=".MovimientosActivity" />
        <activity android:name=".DocumentoActivity" />
        <activity android:name=".NotificacionesActivity" />
        <activity android:name=".CedulaActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".DatosActivity"></activity>
    </application>

</manifest>